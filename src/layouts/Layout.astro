    <!--==============================
	  Google Fonts
	============================== -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Outfit:wght@300;400;500;600;700;800;900&family=Roboto:wght@100;300;400;500;700&display=swap" rel="stylesheet">

    <!--==============================
	    All CSS File
	============================== -->
    <!-- Bootstrap -->
    <link rel="stylesheet" href="/assets/css/bootstrap.min.css">
    <!-- Fontawesome Icon -->
    <link rel="stylesheet" href="/assets/css/fontawesome.min.css">
    <!-- Magnific Popup -->
    <link rel="stylesheet" href="/assets/css/magnific-popup.min.css">
    <!-- Slick Slider -->
    <link rel="stylesheet" href="/assets/css/slick.min.css">
    <!-- Theme Custom CSS -->
    <link rel="stylesheet" href="/assets/css/style.css">
    
		<link rel="preconnect" href="https://fonts.googleapis.com">
		<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
		<link href="https://fonts.googleapis.com/css2?family=Lato:wght@400;900&display=swap" rel="stylesheet"> 
        <style is:global>
			@font-face {
					font-family: 'selimaregular';
					src: url('/font/selima_-webfont.woff2') format('woff2'),
							url('/font/selima_-webfont.woff') format('woff');
					font-weight: normal;
					font-style: normal;
			}

			@font-face {
				font-family: 'tajur_puncakregular';
				src: url('/font/tajur_puncak-webfont.woff2') format('woff2'),
						url('/font/tajur_puncak-webfont.woff') format('woff');
				font-weight: normal;
				font-style: normal;
			}
			

		</style>
		
</head>

<body>
    
		<Navbar></Navbar>
		<slot />

    <!-- Scroll To Top -->
    <div class="scroll-top">
        <svg class="progress-circle svg-content" width="100%" height="100%" viewBox="-1 -1 102 102">
            <path d="M50,1 a49,49 0 0,1 0,98 a49,49 0 0,1 0,-98" style="transition: stroke-dashoffset 10ms linear 0s; stroke-dasharray: 307.919, 307.919; stroke-dashoffset: 307.919;"></path>
        </svg>
    </div>

    <!--==============================
    All Js File
============================== -->

<script is:inline>
    window.addEventListener("scroll", function() {
        if (window.scrollY > 100) {
            document.querySelector("#navbartest").classList.add('sm:block')
            document.querySelector("#navbartest").classList.remove('hidden')
        } else {
            document.querySelector("#navbartest").classList.remove('sm:block')
            document.querySelector("#navbartest").classList.add('hidden')
        }
    }, false);

</script>
    <!-- Jquery -->
    <script src="/assets/js/vendor/jquery-3.6.0.min.js" is:inline></script>
    <!-- Slick Slider -->
    <script src="/assets/js/slick.min.js" is:inline></script>
    <!-- Bootstrap -->
    <script src="/assets/js/bootstrap.min.js" is:inline></script>
    <!-- Magnific Popup -->
    <script src="/assets/js/jquery.magnific-popup.min.js" is:inline></script>
    <!-- Counter Up -->
    <script src="/assets/js/jquery.counterup.min.js" is:inline></script>
    <!-- Range Slider -->
    <script src="/assets/js/jquery-ui.min.js" is:inline></script>
    <!-- Isotope Filter -->
    <script src="/assets/js/imagesloaded.pkgd.min.js" is:inline></script>
    <script src="/assets/js/isotope.pkgd.min.js" is:inline></script>

    <!-- Main Js File -->
    <script src="/assets/js/main.js" is:inline></script>
</body>

</html>
